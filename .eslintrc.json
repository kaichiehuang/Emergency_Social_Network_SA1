{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "jest": true,
    "node": true,
    "browser": true
  },
  "extends": "google",
  "rules": {
    "indent": ["error", 4],
    "require-jsdoc": "warn",
    "prefer-const": "warn",
    "spaced-comment": "warn",
    "comma-dangle": "off",
    "camelcase": "off",
    "max-len": "warn",
    "quotes": "warn",
    "valid-jsdoc": "warn",




    "arrow-parens": "warn"
  }
}
